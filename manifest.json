{
    "manifest_version": 3,
    "name": "Stay Focused",
    "description": "A basic pomodoro timer extension to get your work done",
    "version": "0.0.1",
    "icons": {
        "16": "icons/timer-16.png",
        "32": "icons/timer-32.png",
        "48": "icons/timer-48.png",
        "128": "icons/timer-128.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/timer-16.png",
            "32": "icons/timer-32.png",
            "48": "icons/timer-48.png",
            "128": "icons/timer-128.png"
        }
    },
    "permissions": ["storage", "contextMenus", "alarms", "notifications"],
    "background": {
        "service_worker": "service-worker.js"
      }
}